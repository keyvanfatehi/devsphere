<!DOCTYPE html>
<html>
<head>
  <title>Devsphere::Talk</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if notice.present? %>
    <p class="flash notice"><%= notice %></p>
  <% end %>
  <% if alert.present? %>
    <p class="flash alert"><%= alert %></p>
  <% end %>

  <div id="talkwrapper">
    <%= yield %>
  </div>
</body>
</html>
